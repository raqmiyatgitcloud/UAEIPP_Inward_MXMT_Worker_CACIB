<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd"
      autoReload="true"
      throwExceptions="false"
      internalLogLevel="Error" internalLogFile="${Logedir}/nlog-internal.txt">



  <!-- optional, add some variables
  https://github.com/nlog/NLog/wiki/Configuration-file#variables
  -->
  <variable name="Logedir" value="D:\SwathiRekha\CACIB\Log\UAEIPP_Inward_MXMT_Worker"/>
  <variable name="ArchiveDays" value="5"/>
  
  <targets>
	  <target name="NLogMXtoMTConversionWorker" xsi:type="File" layout="${longdate}|${level:uppercase=true}|${message}"
		fileName="${Logedir}./NLogMXtoMTConversionWorker.txt" archiveAboveSize ="2000000"
		archiveFileName="${Logedir}./Archives/NLogMXtoMTConversionWorker.{#}.txt"
		archiveDateFormat="yyyy-MM-dd HH_mm_ss" archiveNumbering="Date" maxArchiveDays="${ArchiveDays}"
		concurrentWrites="true" keepFileOpen="true" openFileCacheTimeout = "30"/>

    <target name="Service" xsi:type="File" layout="${longdate}|${level:uppercase=true}|${message}"
		fileName="${Logedir}./Service.txt" archiveAboveSize ="2000000"
		archiveFileName="${Logedir}./Archives/Service.{#}.txt"
		archiveDateFormat="yyyy-MM-dd HH_mm_ss" archiveNumbering="Date" maxArchiveDays="${ArchiveDays}"
		concurrentWrites="true" keepFileOpen="true" openFileCacheTimeout = "30"/>
  </targets>
  <rules>
    <logger name="NLogMXtoMTConversionWorker" minlevel="Trace" writeTo="NLogMXtoMTConversionWorker" />
    <logger name="ServiceLog" minlevel="Trace" writeTo="Service" />
  </rules>
	
  <extensions>
    <add assembly="NLog.Web.AspNetCore"/>
  </extensions>
</nlog>
